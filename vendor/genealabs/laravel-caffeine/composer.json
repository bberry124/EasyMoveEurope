{
    "name": "genealabs/laravel-caffeine",
    "description": "Keeping Your Laravel Forms Awake",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/routing": "^9.0",
        "illuminate/support": "^9.0",
        "jenssegers/model": "^1.5"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^7.0",
        "orchestra/testbench-dusk": "^7.0",
        "orchestra/testbench": "^7.0",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^9.5",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "source"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelCaffeine\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
